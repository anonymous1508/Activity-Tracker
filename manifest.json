{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "description": "Tracks time spent on each website and blocks specified websites.",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "settings.html"
}
